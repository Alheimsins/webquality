{
  "name": "@alheimsins/webquality",
  "description": "Test your site using Lighthouse and Security Headers",
  "version": "2.0.2",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=12.16.1"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "web",
    "quality",
    "test",
    "lighthouse",
    "securityheaders"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alheimsins/webquality.git"
  },
  "bugs": {
    "url": "https://github.com/Alheimsins/webquality/issues"
  },
  "homepage": "https://github.com/Alheimsins/webquality#readme",
  "devDependencies": {
    "ava": "3.7.1",
    "coveralls": "3.0.11",
    "husky": "4.2.5",
    "nyc": "15.0.1",
    "standard": "14.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "dependencies": {
    "axios": "0.19.2"
  }
}
